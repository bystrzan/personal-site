{
  "@context": "https://schema.org",
  "@graph": [
    {
      "@type": "WebSite",
      "@id": "{{site.url}}{{page.url}}#website",
      "url": "{{site.url}}{{page.url}}",
      "name": "{{pageTitle}}",
      "description": "{{pageDescription}}"
    },
    {
      "@type": "WebPage",
      "@id": "{{site.url}}{{page.url}}#webpage",
      "url": "{{site.url}}{{page.url}}",
      "inLanguage": "en-US",
      "name": "{{pageTitle}}",
      "isPartOf": {
        "@id": "{{site.url}}{{page.url}}#website"
      },
      "datePublished": "{{ "now" | date: '%Y-%m-%d' | append: 'T00:00:00.000Z' }}",
      "dateModified": "{{ "now" | date: '%Y-%m-%d' | append: 'T00:00:00.000Z' }}"
    }
  ]
}
