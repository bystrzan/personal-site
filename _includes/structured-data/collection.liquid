{
  "@context": "https://schema.org",
  "@graph": [
    {
      "@type": "WebSite",
      "@id": "{{site.url}}{{page.url}}#website",
      "url": "{{site.url}}{{page.url}}",
      "name": "{{pageTitle}}",
      "description": "{{pageDescription}}",
    },
    {
      "@type": "CollectionPage",
      "@id": "{{site.url}}{{page.url}}#webpage",
      "url": "{{site.url}}{{page.url}}",
      "inLanguage": "en-US",
      "name": "{{pageTitle}}",
      "isPartOf": {
        "@id": "{{site.url}}/#website"
      }
    }
  ]
}
